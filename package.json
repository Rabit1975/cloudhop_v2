{
  "name": "cloudhop",
  "version": "2.0.0",
  "description": "CloudHop - A modular operating system for next-generation communication",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run electron:start\"",
    "dev": "vite --host 127.0.0.1",
    "dev:watch": "vite --host 127.0.0.1 --watch",
    "build": "tsc && tsc -p tsconfig.electron.json && vite build",
    "electron:build": "tsc -p tsconfig.electron.json",
    "electron:start": "npm run electron:build && electron .",
    "type-check": "tsc --noEmit",
    "server:dev": "tsx server/index.ts",
    "server:build": "tsc server/index.ts --outDir dist/server"
  },
  "keywords": [
    "cloudhop",
    "os",
    "electron",
    "geforce-now"
  ],
  "author": "CloudHop Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@google/genai": "^1.40.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-three/fiber": "^8.15.0",
    "@supabase/supabase-js": "^2.90.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@types/three": "^0.160.0",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^5.2.1",
    "framer-motion": "^12.27.1",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.160.0",
    "trae": "^0.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.6",
    "electron": "^40.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "prettier": "^3.8.1",
    "supabase": "^2.76.6",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  }
}
