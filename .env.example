# CloudHop 2.0 Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# rabbit.ai Configuration (the only AI layer)
VITE_RABBIT_AI_ENDPOINT=http://localhost:3000/api/rabbit

# WebRTC Signaling Server (Optional)
VITE_SIGNALING_SERVER_URL=your_signaling_server_url_here

# App Configuration
VITE_APP_NAME=CloudHop OS
VITE_APP_VERSION=2.0.0
